{
    "builds": [
        {
            "src": "webpack.config.js",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}